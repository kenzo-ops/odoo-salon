<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_salon_booking_calendar" model="ir.ui.view">
    <field name="name">Salon Booking Calendar</field>
    <field name="model">salon.booking</field>
    <field name="arch" type="xml">
        <calendar string="Jadwal Booking Klinik" date_start="booking_date" color="customer">
            <field name="booking_id"/>
            <field name="customer"/>
        </calendar>
    </field>
    </record>

    <record id="salon_booking_calendar" model="ir.actions.act_window">
        <field name="name">Kalender Salon</field>
        <field name="res_model">salon.booking</field>
        <field name="view_mode">calendar</field>
        <field name="view_id" ref="view_salon_booking_calendar"/>
        <field name="context">{}</field>
    </record>

</odoo>
